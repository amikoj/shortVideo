<?xml version="1.0" encoding="utf-8"?>
<resources>

	<!-- HeaderBar自定义属性 -->
	<declare-styleable name="HeadBarView">
		<attr name="bgColor" format="color" />
		<attr name="leftIcon" format="reference" />
		<attr name="leftText" format="string" />
		<attr name="leftTextColor" format="color" />
		<attr name="hideLeftIcon" format="boolean"/>
		<attr name="title" format="string" />
		<attr name="titleColor" format="color" />
		<attr name="rightText" format="string" />
		<attr name="rightTextColor" format="color" />
		<attr name="rightIcon" format="reference" />
		<attr name="isShowBottomLine" format="boolean" />
		<attr name="bottomLineColor" format="color" />
		<attr name="bottomLineHeight" format="dimension"/>
		<attr name="onLeftClick" format="string"/>
		<attr name="onRightClick" format="string"/>
	</declare-styleable>

</resources>